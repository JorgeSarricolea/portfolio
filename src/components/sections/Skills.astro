---
import tags from "@/js/objects/tags.js";
---

<section class="flex flex-col justify-between gap-2 mt-2">
  <h1 class="text-xl font-bold">Skills</h1>
  <div class="flex flex-wrap gap-1">
    {
      Object.values(tags).map((tag) => (
        <div id="tags-cv" class="flex relative text-black">
          <h3 class="text-md capitalize">{tag.name} |</h3>
        </div>
      ))
    }
  </div>
</section>
